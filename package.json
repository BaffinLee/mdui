{
  "private": true,
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "clean:shared": "rimraf packages/shared/{variables,controllers,decorators,directives,helpers,mixins} packages/shared/*.{js,js.map,d.ts,d.ts.map} packages/shared/variables.less",
    "clean:jq": "rimraf packages/jq/{functions,methods,shared,static} packages/jq/*.{js,js.map,d.ts,d.ts.map}",
    "clean:icons": "rimraf packages/icons/src/*.ts packages/icons/src/shared/style.ts packages/icons/*.{js,d.ts}",
    "clean:components": "rimraf packages/components/{button,checkbox,icon} packages/components/*.{js,js.map,d.ts,d.ts.map} packages/components/src/**/*style.ts",
    "clean:functions": "rimraf packages/functions/*.{js,js.map,d.ts,d.ts.map}",
    "clean:mdui": "rimraf packages/mdui/{components,functions} packages/mdui/*.{js,js.map,d.ts,d.ts.map} packages/mdui/mdui.css",
    "clean": "run-p clean:*",
    "lint:shared": "stylelint --fix packages/shared/src/**/*.less && eslint --fix packages/shared/src/**/*.ts",
    "lint:jq": "eslint --fix packages/jq/src/**/*.ts",
    "lint:icons": "stylelint --fix packages/icons/src/shared/**/*.less && eslint --fix packages/icons/src/shared/**/*.ts",
    "lint:components": "stylelint --fix packages/components/src/**/*.less && eslint --fix packages/components/src/**/*.ts",
    "lint:functions": "eslint --fix packages/functions/src/**/*.ts",
    "lint:mdui": "stylelint --fix packages/mdui/src/styles/**/*.less && eslint --fix packages/mdui/src/{components,functions}/*.ts",
    "lint": "run-p lint:*",
    "analyze:components": "cem analyze --config \"scripts/components/custom-elements-manifest.js\"",
    "analyze:icons": "cem analyze --config \"scripts/icons/custom-elements-manifest.js\"",
    "vscode:components": "node scripts/components/build-vscode-data.js",
    "vscode:icons": "node scripts/icons/build-vscode-data.js",
    "webtypes:components": "node scripts/components/build-web-types.js",
    "webtypes:icons": "node scripts/icons/build-web-types.js",
    "build:shared": "node scripts/shared/build-less.js && node scripts/shared/build-lit-style.js && tsc -p packages/shared/tsconfig.json && node scripts/shared/build-js.js ",
    "build:jq": "tsc -p packages/jq/tsconfig.json",
    "build:icons": "node scripts/icons/build-lit-style.js && node scripts/icons/make-icons.js && tsc -p packages/icons/tsconfig.json && node scripts/icons/build-js.js && npm-run-all -s analyze:icons -p vscode:icons webtypes:icons",
    "build:components": "node scripts/components/build-lit-style.js && tsc -p packages/components/tsconfig.json && node scripts/components/build-js.js && npm-run-all -s analyze:components -p vscode:components webtypes:components",
    "build:functions": "tsc -p packages/functions/tsconfig.json",
    "build:mdui": "tsc -p packages/mdui/tsconfig.json && node scripts/mdui/build-less.js",
    "build": "npm-run-all -p clean lint -s build:jq build:shared build:icons build:components build:functions build:mdui",
    "build-dev": "npm-run-all -p clean lint -s build:jq build:shared \"build:icons --dev\" \"build:components --dev\" build:functions \"build:mdui --dev\"",
    "bundle": "npm run build && rollup -c scripts/mdui/rollup.config.js",
    "test:jq": "eslint --fix packages/jq/__test__/**/*.ts && tsc -p packages/jq/__test__/tsconfig.json && web-test-runner --config scripts/jq/web-test-runner.config.mjs",
    "test:components": "eslint --fix packages/components/__test__/**/*.ts && tsc -p packages/components/__test__/tsconfig.json && web-test-runner --config scripts/components/web-test-runner.config.mjs",
    "test": "run-s test:*",
    "watch:shared": "tsc --watch --preserveWatchOutput -p packages/shared/tsconfig.json",
    "watch:jq": "tsc --watch --preserveWatchOutput -p packages/jq/tsconfig.json",
    "watch:components": "tsc --watch --preserveWatchOutput -p packages/components/tsconfig.json",
    "watch:functions": "tsc --watch --preserveWatchOutput -p packages/functions/tsconfig.json",
    "watch:mdui": "tsc --watch --preserveWatchOutput -p packages/mdui/tsconfig.json",
    "dev:shared": "node scripts/shared/dev.js --dev",
    "dev:components": "node scripts/components/dev.js --dev",
    "dev:mdui": "node scripts/mdui/dev.js --dev",
    "dev-only": "concurrently -k -r \"npm:watch:*\" \"npm:dev:*\" \"vite --config scripts/vite.config.js --host\"",
    "dev": "run-s build-dev dev-only",
    "pack": "npm run build && npm pack --workspaces",
    "export-png-icons": "node scripts/icons/export-png.js"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.6.4",
    "@material-design-icons/svg": "^0.14.2",
    "@open-wc/testing": "^3.1.4",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.1",
    "@types/jquery": "^3.5.14",
    "@types/mocha": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@web/dev-server-rollup": "^0.3.19",
    "@web/test-runner": "^0.15.0",
    "@web/test-runner-playwright": "^0.9.0",
    "autoprefixer": "^10.4.8",
    "clean-css": "^5.2.4",
    "concurrently": "^7.3.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-lit": "^1.6.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jquery": "^3.6.0",
    "less": "^4.1.2",
    "less-plugin-npm-import": "^2.1.0",
    "lint-staged": "^13.0.3",
    "minify-html-literals": "^1.3.5",
    "node-watch": "^0.7.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-less": "^6.0.0",
    "pretender": "^3.4.7",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.9.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "stylelint": "^14.10.0",
    "stylelint-config-standard": "^29.0.0",
    "svgexport": "^0.4.2",
    "typescript": "^4.7.4",
    "vite": "^4.0.4",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.less": [
      "stylelint --fix"
    ],
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.19.0",
    "pnpm": ">=7"
  }
}
